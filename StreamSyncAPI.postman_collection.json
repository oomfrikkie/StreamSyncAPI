    {
      "name": "Account",
      "item": [
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"derjen@email.com\",\n  \"first_name\": \"Derjen\",\n  \"last_name\": \"Frick\",\n  \"password\": \"strongpassword123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/account/register",
              "host": ["{{baseUrl}}"],
              "path": ["account", "register"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200 or 201\", function () {",
                  "    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
                  "});",
                  "pm.test(\"Response has verification_token\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('verification_token');",
                  "});",
                  "pm.collectionVariables.set(\"verification_token\", pm.response.json().verification_token);"
                ]
              }
            }
          ]
        },
